URxvt.font:     xft:DejaVu Sans Mono:size=11
URxvt.boldFont: xft:DejaVu Sans Mono:bold:size=11

urxvt*termName:         rxvt
urxvt*background:       #0c1014
urxvt*foreground:       #99d1ce
urxvt*scrollBar:        false
urxvt*matcher.button:   1
urxvt.transparent:    false
urxvt*allow_bold:    true

Xft*dpi:                96
Xft*antialias:          true
Xft*hinting:            full

URxvt*geometry: 85x16
URxvt*fading:        0
URxvt*tintColor:     #ffffff
URxvt*shading:        0
URxvt*inheritPixmap:     False

! black + red
!urxvt*color0:     #3f3f3f
urxvt*color0:      #0c1014
urxvt*color1:      #c23127
! green + yellow
urxvt*color2:      #2aa889
urxvt*color3:      #edb443
! blue + purple
urxvt*color4:      #195466
urxvt*color5:      #4e5166
! cyan + white
urxvt*color6:      #33859e
urxvt*color7:      #99d1ce
! bright-black + bright-red
urxvt*color8:      #11151c
urxvt*color9:      #d26937
! bright-green + bright-yellow
urxvt*color10:     #2aa889
urxvt*color11:     #245361
! bright-blue + bright-purple
urxvt*color12:     #195466
urxvt*color13:     #4e5166
! bright-cyan + bright-white
urxvt*color14:     #599cab
urxvt*color15:     #d3ebe9
!}}}

! xscreensaver ---------------------------------------------------------------

!general dialog box (affects main hostname, username, password text)
xscreensaver.Dialog.foreground:         #7f8f9f
xscreensaver.Dialog.background:         #0c0d0e
xscreensaver.Dialog.topShadowColor:     #0c0d0e
xscreensaver.Dialog.bottomShadowColor:  #0c0d0e
xscreensaver.Dialog.Button.foreground:  #0c0d0e
xscreensaver.Dialog.Button.background:  #7f8f9f
!username/password input box and date text colour
xscreensaver.Dialog.text.foreground:    #0c0d0e
xscreensaver.Dialog.text.background:    #7f8f9f
xscreensaver.Dialog.internalBorderWidth:24
xscreensaver.Dialog.borderWidth:        20
xscreensaver.Dialog.shadowThickness:    2
!timeout bar (background is actually determined by Dialog.text.background)
xscreensaver.passwd.thermometer.foreground:  #7f8f9f
xscreensaver.passwd.thermometer.background:  #0c0d0e
xscreensaver.passwd.thermometer.width:       10
!datestamp format--see the strftime(3) manual page for details
xscreensaver.dateFormat:    %I:%M%P %a %b %d, %Y
